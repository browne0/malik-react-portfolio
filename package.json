{
  "name": "malik-react-portfolio",
  "version": "0.1.0",
  "homepage": "http://malikbrowne.com",
  "private": true,
  "devDependencies": {
    "react-scripts": "1.0.11"
  },
  "dependencies": {
    "contentful": "^4.6.2",
    "github-markdown-css": "^2.8.0",
    "material-design-icons-iconfont": "^3.0.3",
    "material-ui": "^0.18.7",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "prismjs": "^1.7.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-flip-move": "^2.9.15",
    "react-ga": "^2.2.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "^2.5.0",
    "react-moment": "^0.6.2",
    "react-router-dom": "^4.1.2",
    "react-slick": "^0.15.4",
    "react-snapshot": "^1.2.0",
    "react-syntax-highlighter": "^5.7.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-toastr": "^2.9.4",
    "react-transition-group": "^2.2.0",
    "source-map-explorer": "^1.5.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass src/base.scss -o src/styles/",
    "watch-css": "npm run build-css && node-sass src/base.scss -o src/styles/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
