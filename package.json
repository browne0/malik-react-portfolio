{
  "name": "malik-react-portfolio",
  "version": "0.1.0",
  "homepage": "http://dev.beesdesign.net/malikbrowne",
  "private": true,
  "devDependencies": {
    "react-scripts": "1.0.11"
  },
  "dependencies": {
    "github-markdown-css": "^2.8.0",
    "markdown-with-front-matter-loader": "^0.1.0",
    "material-ui": "^0.18.7",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-grid-gallery": "^0.4.2",
    "react-router-dom": "^4.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-transition-group": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
